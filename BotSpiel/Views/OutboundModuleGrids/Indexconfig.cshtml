

@model IEnumerable<BotSpiel.DataAccess.Models.OutboundModuleGridsconfig>

@{
    ViewBag.Title = "Index Outbound Module Grids - Configuration";
}
<br>
<h2>Outbound Module Grids - Configuration</h2>






@(Html
        .Grid(Model)
        .Build(columns =>
        {
				columns.Add(model => model.ixOutboundModuleGrid).Titled("Grids").Encoded(false).RenderedAs(item => Html.ActionLink("Grid", "Index", item.sOutboundModuleGrid));
				columns.Add(model => model.sOutboundModuleGrid).Titled("Outbound Module Grid").Sortable(true).Filterable(true);
				columns.Add(model => model.sShortDescription).Titled("Short Description").Sortable(true).Filterable(true);

        })
        .Pageable(pager =>
        {
            pager.RowsPerPage = 20;
        })
		//.Filterable()
 )



 

