

@model IEnumerable<BotSpiel.DataAccess.Models.FoundationModuleGridsmd>

@{
    ViewBag.Title = "Index Foundation Module Grids - Master Data";
}
<br>
<h2>Foundation Module Grids - Master Data</h2>






@(Html
        .Grid(Model)
        .Build(columns =>
        {
				columns.Add(model => model.ixFoundationModuleGrid).Titled("Grids").Encoded(false).RenderedAs(item => Html.ActionLink("Grid", "Index", item.sFoundationModuleGrid));
				columns.Add(model => model.sFoundationModuleGrid).Titled("Foundation Module Grid").Sortable(true).Filterable(true);
				columns.Add(model => model.sShortDescription).Titled("Short Description").Sortable(true).Filterable(true);

        })
        .Pageable(pager =>
        {
            pager.RowsPerPage = 20;
        })
		//.Filterable()
 )



 

