

@model IEnumerable<BotSpiel.DataAccess.Models.NetworkModuleGridsmd>

@{
    ViewBag.Title = "Index Network Module Grids - Master Data";
}
<br>
<h2>Network Module Grids - Master Data</h2>






@(Html
        .Grid(Model)
        .Build(columns =>
        {
				columns.Add(model => model.ixNetworkModuleGrid).Titled("Grids").Encoded(false).RenderedAs(item => Html.ActionLink("Grid", "Index", item.sNetworkModuleGrid));
				columns.Add(model => model.sNetworkModuleGrid).Titled("Network Module Grid").Sortable(true).Filterable(true);
				columns.Add(model => model.sShortDescription).Titled("Short Description").Sortable(true).Filterable(true);

        })
        .Pageable(pager =>
        {
            pager.RowsPerPage = 20;
        })
		//.Filterable()
 )



 

